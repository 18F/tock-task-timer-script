{
  "name": "tock-play",
  "version": "1.0.0",
  "description": "",
  "main": "tock-play.js",
  "watch": {
    "build": "tock-play.js"
  },
  "scripts": {
    "build": "uglifyjs tock-play.js --screw-ie8 --mangle --compress --output tock-play.min.js && npm run urlencode && npm run addjs",
    "urlencode": "node -e \"require('fs').writeFileSync('tock-play.min.js', encodeURIComponent(require('fs').readFileSync('tock-play.min.js')))\"",
    "addjs": "echo \"javascript:$(cat tock-play.min.js)\\c\" > tock-play.min.js",
    "watch": "npm-watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-watch": "^0.1.6",
    "uglify-js": "^2.7.4"
  }
}
